function _draw()
	cls()
	rectfill(0,0,128,128,1)
	local dx = 0
	local dy = mid(-4.9, cos(time() / 8) * 5, 4.9)
	
	--print(dy, 12,12, 0)
	
	spr(37, 46 + dx, 10 + dy, 5, 8)
	
	for x=0,128 do
		pset(x, sin(time() / 2) + 100 + sin(x/64) * 5, 12)
	end
	
end
