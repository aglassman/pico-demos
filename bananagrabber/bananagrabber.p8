#include ../demos/animation.lua
banana_count=0
banana_goal=5
banana_counter=nil
banana_obj=nil
player=nil
sg=nil
falling_jorts=nil
floor=50
landing=nil
bg={
  sprite=0,
  x=0,
  y=0,
  
  new=function(self, args)
   return setmetatable(args, {__index=self})
  end,

  draw=function(self)
   spr(self.sprite, self.x, self.y, 2, 4)
  end,

  update=function(self)

  end,

  update_position=function(self, source, x, y)
    if source == "segway" then
     self.x = x
     self.y = y - 16
    end 
  end 

}

segway={
  sprite_backward=66,
  sprite_neutral=64,
  sprite_forward=68,
  friction=.9,
  acc=.2,
  
  x=0,
  y=0,
  vx=0,
  vy=0,
  gy=1,
  rider=nil,
  last_update=nil,

  new=function(self, args)
   local new_segway = {
    x=args.x,
    y=args.y,
    vx=0,
    vy=0,
    rider=nil,
    last_update=time(),
    current_sprinte=nil,
    jumping=false,
   }

   return setmetatable(new_segway, {__index=self})
  end,

  update=function(self)
   if (btn(‚¨ÖÔ∏è)) then
    self.vx -= self.acc
   end 

   if (btn(‚û°Ô∏è)) then
    self.vx += self.acc
   end 

   if (floor > 30 and btnp(‚¨ÜÔ∏è)) then
    self.moveup=true
    self.vy = -7
    self.jumping=true
    floor-=20
   end 

   if (floor < 110 and btnp(‚¨áÔ∏è)) then
    floor+=20
   end

   if(btnp(‚ùé) and not self.jumping) then
    self.vy = -7
    self.jumping=true
   end

   self.x += self.vx
   self.vx *= self.friction

   self.vy += self.gy
   self.y += self.vy

   if self.jumping and self.moveup and self.vy > -.5 then
    self.y = floor
    self.vy = 0
    self.jumping=false
    self.moveup=false
   elseif not self.moveup and self.y > floor then
    self.y = floor
    self.vy = 0
    self.jumping=false
   else
     

   end 

   if self.rider then
    self.rider:update_position("segway", self.x, self.y)
   end 

  end, 

  draw=function(self)
  
   if (time() - self.last_update) > .1 then
    self.last_update = time()
    if self.vx > 0.05 then
     self.current_sprite = self.sprite_forward
    elseif self.vx < - 0.05 then
     self.current_sprite = self.sprite_backward
    else
     self.current_sprite = self.sprite_neutral
    end
  end  

  local debug_x = flr(self.x / 8)
  local debug_y = flr(self.y  + 16 / 8)

   spr(self.current_sprite, self.x, self.y, 2, 2)
   --print(floor, 3, 10, 2)
   -- print(self.gy, 3, 10, 2)
   -- print(self.vy, 3, 20, 2)
   -- print(self.y, 3, 30, 2)
   -- print(mget(debug_x,debug_y), 3, 40, 2)
   -- pset(self.x,self.y + 16, 8)
   -- print(debug_x .. "," .. debug_y, self.x - 16, self.y + 16, 1)
  end,

}

banana_stand={
 x=0,
 y=0,

 draw=function(self)
  spr(72, self.x, self.y, 4, 4)
  spr(76, self.x, self.y + 32, 4, 2)
 end 
}

banana={
   new=function(self, args)
   local new_banana = {
    position={args.x, args.y},
    pulse=false
   }

   return setmetatable(new_banana, {__index=self})
  end,

  draw=function(self, scale)
   --spr(13, self.x, self.y, 2, 2)
   local scale = nil
   if self.pulse then
    scale = abs(sin(time()))
   else
    scale = 1
   end

   local x= self.position[1] - (8 * scale)
   local y= self.position[2] - (8 * scale)
   sspr(13 * 8,0,16,16,x, y, 16 * scale, 16 * scale)
  end,

  _draw=function(self, x, y)
    spr(13, x, y, 2, 2)
  end, 

  throw=function(self)
   local y1 =  rnd() * 100
   local y2 =  0
   local y3 =  rnd() * 100

   local land_x = rnd() * 50
   local land_y = 20 + (rnd() * 100)

   landing = {x=land_x, y=land_y}

   animate(self, {
    pipeline={
     translate(-64, -64),
     q_bezier({{x=land_x,y=land_y},{x=80,y=y2},{x=120,y=80}})},
     --draw_path_fn=q_bezier_draw({{x=land_x,y=land_y},{x=80,y=y2},{x=120,y=80}}),
     duration=1,
     reverse=true
   })
  end 
}

jorts={
  new=function(self, args)
   local new_jorts = {
    x=args.x,
    y=args.y
   }

   return setmetatable(new_jorts, {__index=self})
  end,

  draw=function(self, scale)
   spr(194,self.x, self.y + -abs(sin(time())) * 10, 2, 2)
  end,

  hop=function(self)
    self.x = self.x - 1
    if self.x < - 10 then
      self.x = 200
      self.y = rnd() * 128
    end
  end 
}

function q_bezier_draw(points)
 local q_bz = q_bezier(points)
 return function()
  n = 200
  for i=0, n, 1 do
   local t = i / n
   local x, y = q_bz(t)
   pset(x, y, 7)
  end 

  for p in all(points) do
   pset(p.x, p.y, 8)
  end
 end 
end

function _init()
 sg = segway:new({x=50, y=50})
 player = bg:new({x=50, y=50})
 sg.rider = player
 banana_stand.x=100
 banana_stand.y=50
 banana_counter = banana:new({x=10, y=10 })
 falling_jorts = jorts:new({x=20, y=20 })
 banana_obj = banana:new({x=64, y=64 })
end

function _update()
 sg:update()
 player:update()
 falling_jorts:hop()

 if (btnp(üÖæÔ∏è)) then
   banana_obj:throw()
 end 

end

function _draw()
 -- clear and raw map
 cls()
 palt(11, true)
 palt(0, false)
 -- blue sky
 -- green ground
 rectfill(0,0,128,128,3)
 rectfill(0,0,128,30,12)
 map(0,0,0,0,16,16)

 if landing then
   ovalfill(landing.x + 8,landing.y + 8, landing.x + 8 + 10, landing.y + 8 + 3, 1)
 end 

 -- player
 player:draw()
 sg:draw()

 -- banana counter
 banana_counter:draw(1)
 print(banana_count.." of "..banana_goal, 20, 7, 2)

 

 banana_stand:draw()

 -- obstacles 
 falling_jorts:draw()
 draw_animated(banana_obj)

end

